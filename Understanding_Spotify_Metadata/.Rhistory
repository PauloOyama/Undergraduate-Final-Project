# Guarda o resultado do melhor filho em 1000 epocas
periodos <- list()
# Guarda o numero de filhos
mu <- c(1,5,10,20,50,100)
for(i in mu){
for(j in 1:1000){
x0 = runif(n = i, min = -0.2, max =0.2)
epoca <- ee(tmax=1, x = x0, n = length(x0))
# O primeiro disparo é o melhor filho no periodo
if (j == 1){
periodos[[i]] = epoca
}else{
# O melhor filho dessa epoca eh melhor que o filho
# do periodo ?
# Se sim, troca o filho
if(max(periodos[[i]]$f) < max(epoca$f)){
periodos[[i]] <- epoca
periodos[[i]]$count <- j
}
}
}
}
